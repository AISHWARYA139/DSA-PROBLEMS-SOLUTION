#include<iostream>
using namespace std;
int main(){
 int n;
 cin>>n;
int  row=1;
char start ='A';
while(row<=n){
     int col=1;
      while(col<=row){
          //char ch= 'A'+row+col-2;
         cout<<start<<" ";
         start++;
         col++;
         
     }
     cout<<endl;
     row++;
 }
 }

op:
A 
B C 
D E F 
