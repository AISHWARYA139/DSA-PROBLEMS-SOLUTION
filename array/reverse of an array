#include<bits/stdc++.h>
using namespace std;
void reversearr(int arr[],int n){
    int st=0;
    int en=n-1;
    while(st<=en){
        swap(arr[st],arr[en]);
        st++;
        en--;
    
    
    }
}
void printarr(int arr[], int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    
}
int main() {
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    reversearr(arr,n);
    printarr(arr,n);
}

